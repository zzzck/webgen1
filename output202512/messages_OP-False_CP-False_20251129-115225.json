 </div>\n\n</div>\n\n<div class=\"buy-bar\" onclick=\"openModal()\">ç«‹å³è´­ä¹°</div>\n\n<div class=\"modal-overlay\" id=\"modal\">\n  <div class=\"modal-content\">\n    <h3>ç¡®è®¤è®¢å•</h3>\n    <p id=\"selectedInfo\"></p>\n    <div class=\"final-price\" id=\"finalPrice\"></div>\n    <button onclick=\"confirmPay()\">ç¡®è®¤æ”¯ä»˜</button>\n  </div>\n</div>\n\n<script>\nlet countdownTarget = new Date();\ncountdownTarget.setHours(countdownTarget.getHours() + 2);\nfunction updateCountdown(){\n    let now = new Date();\n    let diff = countdownTarget - now;\n    if(diff <= 0){\n        document.getElementById(\"countdown\").textContent = \"æ´»åŠ¨å·²ç»“æŸ\";\n        return;\n    }\n    let h = Math.floor(diff / 1000 / 60 / 60);\n    let m = Math.floor((diff / 1000 / 60) % 60);\n    let s = Math.floor((diff / 1000) % 60);\n    document.getElementById(\"countdown\").textContent = `${h}å°æ—¶${m}åˆ†${s}ç§’`;\n}\nsetInterval(updateCountdown,1000);\nupdateCountdown();\n\nfunction changeImage(src){\n    document.getElementById(\"mainImage\").src = src;\n}\n\nlet selectedCoupon = null;\ndocument.querySelectorAll('.coupon').forEach(c => {\n    c.addEventListener('click', function(){\n        selectedCoupon = { min: parseFloat(c.dataset.min), discount: parseFloat(c.dataset.discount) };\n        document.querySelectorAll('.coupon').forEach(cc=>cc.style.background='#fff7f7');\n        c.style.background='#ffe8e8';\n    });\n});\n\nfunction getFinalPrice(){\n    let select = document.getElementById('packageSelect');\n    let price = parseFloat(select.options[select.selectedIndex].dataset.price);\n    if(selectedCoupon && price >= selectedCoupon.min){\n        price -= selectedCoupon.discount;\n    }\n    return price.toFixed(2);\n}\n\nfunction openModal(){\n    let select = document.getElementById('packageSelect');\n    let amount = select.value;\n    let basePrice = select.options[select.selectedIndex].dataset.price;\n    let info = `è‘µèŠ±ç±½ ${amount}åŒ…ï¼Œå•ä»·æ€»è®¡ Â¥${basePrice}${selectedCoupon ? `ï¼Œä½¿ç”¨ä¼˜æƒ åˆ¸æ»¡${selectedCoupon.min}å‡${selectedCoupon.discount}` : \"\"}`;\n    document.getElementById('selectedInfo').textContent = info;\n    document.getElementById('finalPrice').textContent = \"æœ€ç»ˆä»·æ ¼ï¼šÂ¥\" + getFinalPrice();\n    document.getElementById('modal').style.display = 'flex';\n}\n\nfunction confirmPay(){\n    let order = {\n        product: \"å†…è’™å¤è‘µèŠ±ç±½\",\n        package: document.getElementById('packageSelect').value,\n        coupon: selectedCoupon,\n        finalPrice: getFinalPrice()\n    };\n    alert(\"è®¢å•ç¡®è®¤ï¼š\" + JSON.stringify(order));\n    document.getElementById('modal').style.display = 'none';\n}\n\ndocument.getElementById('modal').addEventListener('click', function(e){\n    if(e.target.id === 'modal') this.style.display = 'none';\n});\n\ndocument.getElementById('reviewFilter').addEventListener('change', function(){\n    let val = this.value;\n    document.querySelectorAll('#reviewList .review-item').forEach(item => {\n        if(val === 'all' || item.dataset.type === val){\n            item.style.display = 'block';\n        } else {\n            item.style.display = 'none';\n        }\n    });\n});\n</script>\n</body>\n</html>"
